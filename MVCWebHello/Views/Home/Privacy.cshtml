@{
    ViewData["Title"] = "Statisztika";
}
<p style="text-align:right">Belépve, mint: <span id="cookies"></span></p>
<h1>@ViewData["Title"]</h1>


<p class="zold">Eddig belépett felhasználók</p>

<table class="table table-striped table-hover">
  <thead>
    <tr>     
      <th scope="col">Belépés ideje</th>
      <th scope="col">Név</th>
      <th scope="col">Ip cím</th>
    </tr>
  </thead>
  <tbody>
       @{for(var i = 0; i< @Model.Szamlalo; i++){
       <tr>            
            <td>@Model.Nevek[i]</td>
            <td>@Model.Idok[i]</td>
            <td>@Model.Ipk[i]</td>
       </tr>
  }} 
  </tbody>
</table>